{
  "name": "limbus-combat-guide",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:css": "tailwindcss -i src/app/styles/app.css -o public/static/app.css",
    "watch:css": "tailwindcss -i src/app/styles/app.css -o public/static/app.css --watch",
    "start": "next start",
    "lint": "next lint",
    "build:worker": "npx opennextjs-cloudflare build",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "npm run build:worker && npm run dev:worker",
    "deploy:worker": "npm run build:worker && wrangler deploy"
  },
  "dependencies": {
    "bootstrap": "^5.3.5",
    "next": "^15.2.4",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@opennextjs/cloudflare": "^1.0.0-beta.0",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^20.17.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "wrangler": "^4.7.2"
  }
}
